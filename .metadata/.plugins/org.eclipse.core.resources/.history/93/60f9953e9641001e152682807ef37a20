#include "Queue_Handler.h"

static char pData[1];

void Queue_Handler(){
	HAL_UART_Receive_IT(&huart1, (uint8_t*)&pData, (uint16_t)sizeof(pData));
}

void HAL_UART_RxCpltCallback(UART_HandleTypeDef *huart){
	if (pData[0] == '\r'){
		Dequeue();
	}
	Enqueue(&pData[0]);
}
