#ifndef CLI_COMMAND_PARSER_H_
#define CLI_COMMAND_PARSER_H_

#include "main.h"
#include "uart_handler.h"

typedef void (*CommandFunction)();

typedef struct {
    const char *command;
    const char *cond;
    CommandFunction function;
} CommandMap;

void command_parser(char *Command_String);

#endif /* CLI_COMMAND_PARSER_H_ */
