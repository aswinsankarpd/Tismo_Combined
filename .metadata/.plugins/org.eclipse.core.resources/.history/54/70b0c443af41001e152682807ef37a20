#include "calculation.h"
static uint32_t adc_val;
void calculation(){
	adc_val = get_adc_val();
	float resistance = (adc_val*10000)/(5-adc_val);
	HAL_UART_Transmit(&huart1, &resistance, size(float), HAL_MAX_DELAY);
}
