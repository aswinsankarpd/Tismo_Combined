#include "calculation.h"
static uint32_t adc_val;

void calculation(){
	adc_val = get_adc_val();
	int resistance = (adc_val*10000)/(5-adc_val);
	char message[100];
	sprintf(messsage,"The res is %d.",resistance);
	HAL_UART_Transmit(&huart1, (uint8_t*)message, strlen(message),500);
}
