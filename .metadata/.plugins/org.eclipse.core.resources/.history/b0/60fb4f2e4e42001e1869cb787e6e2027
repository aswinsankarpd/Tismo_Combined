#include "pwm_start.h"
static int dutycycle=90;

void pwm_start(){
	  HAL_TIM_PWM_Init(&htim2);
	  HAL_TIM_PWM_Start(&htim2, TIM_CHANNEL_1);
	  dutycycle = getduty();
	  htim2.Instance->CCR4=(uint32_t)50;
//	  __HAL_TIM_SET_COMPARE(&htim2, TIM_CHANNEL_1, dutycycle);
}
